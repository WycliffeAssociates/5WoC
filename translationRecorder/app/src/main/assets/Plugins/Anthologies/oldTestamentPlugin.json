{
    "resource": "bible",
    "anthology": {
        "slug": "ot",
        "name": "old testament",
        "file_conv": "1001111111",
        "parser": {
            "regex": "([a-zA-Z]{2,3}[-[\\d\\w]+]*)_([a-zA-Z]{3})_b([\\d]{2})_([1-3]*[a-zA-Z]+)_c([\\d]{2,3})_v([\\d]{2,3})(-([\\d]{2,3}))?(_t([\\d]{2}))?(.wav)?",
            "groups": {
                "language": 1,
                "version": 2,
                "book_number": 3,
                "book": 4,
                "chapter": 5,
                "start_verse": 6,
                "end_verse": 8,
                "take": 10
            }
        }
    },
    "modes": [
        {
            "name":"verse",
            "type":"single"
        },
        {
            "name":"chunk",
            "type":"multi"
        }
    ],
    "books": "OTBooks.json",
    "chunks": "bibleChunks/",
    "versions": "bibleVersions.json"
}